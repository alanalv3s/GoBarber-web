tItem('@GoBarber:user');\n\n    if (token && user) {\n      return { token, user: JSON.parse(user) };\n    }\n\n    return {} as AuthState;\n  });\n\n  const signIn = useCallback(async ({ email, password }) => {\n    const response = await api.post('sessions', {\n      email,\n      password,\n    });\n\n    const { token, user } = response.data;\n\n    localStorage.setItem('@GoBarber:token', token);\n    localStorage.setItem('@GoBarber:user', JSON.stringify(user));\n\n    setAuthData({ token, user });\n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@GoBarber:token');\n    localStorage.removeItem('@GoBarber:user');\n\n    setAuthData({} as AuthState);\n  }, []);\n\n  return (\n    <AuthContext.Provider value={{ user: authData.user, signIn, signOut }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nfunction useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  if (!context) {\n    throw new Error('useAuth must be used within an AuthProvider');\n  }\n\n  return context;\n}\n\nexport { AuthProvider, useAuth };\n","/home/alan/rocketseat/GoBarber-web/src/hooks/index.tsx",[],"/home/alan/rocketseat/GoBarber-web/src/hooks/toast.tsx",["105","106"],"/home/alan/rocketseat/GoBarber-web/src/components/ToastContainer/Toast/index.tsx",[],"/home/alan/rocketseat/GoBarber-web/src/components/ToastContainer/Toast/styles.ts",[],{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"107","replacedBy":"117"},{"ruleId":"109","replacedBy":"118"},{"ruleId":"111","replacedBy":"119"},{"ruleId":"113","replacedBy":"120"},{"ruleId":"115","replacedBy":"121"},{"ruleId":"122","severity":1,"message":"123","line":17,"column":10,"nodeType":"124","messageId":"125","endLine":17,"endColumn":40},{"ruleId":"122","severity":1,"message":"126","line":13,"column":12,"nodeType":"124","messageId":"125","endLin